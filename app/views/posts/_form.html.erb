<%= render 'shared/errors', obj: @post %>

<%= form_for(@post) do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<br>

	<%= f.label :url %>
	<%= f.text_field :url %>
	<br>

	<%= f.label :description %>
	<%= f.text_area :description %>
	<br>
	<%= f.submit class: "btn btn-primary btn-large"  %>

<% end %>

<%= form_tag '/posts' do %>
	<%= label_tag :olo %>
	<%= text_field_tag :olo %>
	<br>
	<%= submit_tag :submit %>
<% end %>